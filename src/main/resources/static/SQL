create table Patients
(
    id              int generated by default as identity primary key,
    patient_name    varchar(100) not null,
    local_date      date default current_date not null,
    cytology_TOR    boolean,
    hiv_TOR         boolean,
    syphilis_TOR    boolean,
    coroutine_TOR   boolean,
    helminth_TOR    boolean,
    hepatitis_B_TOR boolean,
    hepatitis_C_TOR boolean,
    vidal_TOR       boolean,
    sum             int
)

insert into patients(patient_name, local_date, cytology_tor, hiv_tor, syphilis_tor, coroutine_tor, helminth_tor,
                     hepatitis_b_tor, hepatitis_c_tor, vidal_tor, sum)
VALUES ('Колесников М М','2022-11-08',false,true,true,true,false,true,true,true,1000)

